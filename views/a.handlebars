            <script>
            // The jQuery Way - Works in all browsers but requires jQuery library.

var $playlistLinks = $('.playlist a');

function playSong(event) {
  event.preventDefault();
  var id = $(this).parent().data('id'),
      player = $('.player[data-id="'+id+'"]')[0],
      $currentPlayListLinks = $('.playlist[data-id="'+id+'"] a');
  $currentPlayListLinks.removeClass('active');
  $(this).addClass('active');
  player.src = $(this).attr('href');
  player.play();
}

$playlistLinks.click(playSong);

// The Alternate Vanilla JavaScript Way - No libraries required but doesn't work in IE8 because use of addEventListener instead of attachEvent oh well...

var playListLinks = document.querySelectorAll('.playlist a');

function playSong(event) {
  event.preventDefault();
  var id = this.parentNode.dataset.id,
      player = document.querySelector('.player[data-id="'+id+'"]'),
      currentPlayListLinks = document.querySelectorAll('.playlist[data-id="'+id+'"] a');
   for (i=0; i < currentPlayListLinks.length; i++) {
    currentPlayListLinks[i].classList.remove('active');
  }
  this.classList.add('active');
  player.src = this.href;
  player.play();   
}

for (i=0; i < playListLinks.length; i++) {
  playListLinks[i].addEventListener('click', playSong);
}
